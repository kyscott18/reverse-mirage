{
  "name": "reverse-mirage-monorepo",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install:packages": "pnpm --filter './packages/**' install",
    "preinstall": "npx -y only-allow pnpm",
    "codegen": "pnpm wagmi generate",
    "build": "pnpm --filter 'reverse-mirage' build &&  pnpm --filter 'reverse-mirage-react' build && pnpm --filter !'reverse-mirage' --filter !'reverse-mirage-react' build",
    "lint": "pnpm --parallel lint",
    "format": "pnpm --parallel format",
    "typecheck": "pnpm --parallel typecheck",
    "test": "pnpm --parallel test",
    "test:ci": "pnpm --parallel test:ci",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@wagmi/cli": "^1.5.0",
    "biome": "^0.3.3",
    "typescript": "^5.2.2",
    "vitepress": "1.0.0-rc.10"
  }
}
