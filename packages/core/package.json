{
  "name": "reverse-mirage",
  "version": "0.1.6",
  "description": "Application level typescript utilities for Ethereum",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.bench.ts",
    "!src/test/**/*"
  ],
  "scripts": {
    "bench": "vitest bench",
    "bench:ci": "CI=true vitest bench",
    "build": "tsup",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "CI=true vitest"
  },
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "web3"
  ],
  "author": "Kyle Scott",
  "license": "MIT",
  "repository": "kyscott18/reverse-mirage",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "size-limit": [
    {
      "name": "reverse-mirage (cjs)",
      "path": "./dist/index.cjs",
      "limit": "25 kB"
    },
    {
      "name": "reverse-mirage",
      "path": "./dist/index.js",
      "limit": "20 kB",
      "import": "*"
    }
  ],
  "dependencies": {
    "tiny-invariant": "^1.3.1"
  },
  "peerDependencies": {
    "viem": ">=1"
  },
  "devDependencies": {
    "@uniswap/sdk-core": "^4.0.6",
    "@viem/anvil": "^0.0.6",
    "rome": "^12.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "viem": "^1.5.2",
    "vitest": "^0.34.1"
  },
  "packageManager": "pnpm@8.6.10"
}
