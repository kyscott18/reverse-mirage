{
  "name": "reverse-mirage-react",
  "version": "0.0.4",
  "description": "Application level react typescript utilities for Ethereum",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.bench.ts",
    "!src/test/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "format": "biome format . --write",
    "lint": "biome check .",
    "lint:fix": "pnpm lint --apply",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "web3"
  ],
  "author": "Kyle Scott",
  "license": "MIT",
  "repository": "kyscott18/reverse-mirage",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "size-limit": [
    {
      "name": "reverse-mirage-react (cjs)",
      "path": "./dist/index.cjs",
      "limit": "5 kB"
    },
    {
      "name": "reverse-mirage-react",
      "path": "./dist/index.js",
      "limit": "5 kB",
      "import": "*"
    }
  ],
  "dependencies": {
    "tiny-invariant": "^1.3.1"
  },
  "peerDependencies": {
    "reverse-mirage": "workspace:*",
    "viem": ">=1",
    "wagmi": ">=1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "viem": "^1.6.3",
    "wagmi": "^1.3.10"
  },
  "packageManager": "pnpm@8.6.10"
}
